<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="720.0" prefWidth="1280.0" stylesheets="@stylesheet.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.Controller.FXMLDocumentController">
    <children>
      <HBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <VBox HBox.hgrow="ALWAYS">
               <children>
                  <GridPane VBox.vgrow="ALWAYS">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                     <children>
                        <Pane prefHeight="200.0" prefWidth="200.0" styleClass="background_tile" />
                        <Pane prefHeight="200.0" prefWidth="200.0" styleClass="background_tile" GridPane.columnIndex="1" />
                        <Pane prefHeight="200.0" prefWidth="200.0" styleClass="background_tile" GridPane.columnIndex="2" />
                        <Pane prefHeight="200.0" prefWidth="200.0" styleClass="background_tile" GridPane.columnIndex="3" />
                        <Pane prefHeight="200.0" prefWidth="200.0" styleClass="background_tile" GridPane.columnIndex="4" />
                        <Pane prefHeight="200.0" prefWidth="200.0" styleClass="background_tile" GridPane.columnIndex="5" />
                        <Pane prefHeight="200.0" prefWidth="200.0" styleClass="background_tile" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                        <Pane prefHeight="200.0" prefWidth="200.0" styleClass="background_tile" GridPane.rowIndex="1" />
                        <Pane onMouseClicked="#handlePaneClick" prefHeight="200.0" prefWidth="200.0" styleClass="background_tile_filled" GridPane.columnIndex="2" GridPane.rowIndex="1" />
                        <Pane prefHeight="200.0" prefWidth="200.0" styleClass="background_tile" GridPane.columnIndex="3" GridPane.rowIndex="1" />
                        <Pane prefHeight="200.0" prefWidth="200.0" styleClass="background_tile" GridPane.columnIndex="4" GridPane.rowIndex="1" />
                        <Pane prefHeight="200.0" prefWidth="200.0" styleClass="background_tile" GridPane.columnIndex="5" GridPane.rowIndex="1" />
                     </children>
                  </GridPane>
               </children>
            </VBox>
            <VBox HBox.hgrow="ALWAYS">
               <children>
                    <Button fx:id="button" onAction="#handleButtonAction" text="Run!" />
                    <Label fx:id="label" minHeight="16" minWidth="69" />
               </children>
            </VBox>
         </children>
      </HBox>
    </children>
</AnchorPane>
