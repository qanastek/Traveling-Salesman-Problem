<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" fx:id="ap" prefHeight="600.0" prefWidth="600.0" styleClass="backgroundColor" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.Vues.HomeController">
    <stylesheets>
        <URL value="@stylesheet.css" />
    </stylesheets>
   <children>
      <HBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" HBox.hgrow="ALWAYS">
               <children>
                  <Label alignment="CENTER" contentDisplay="CENTER" maxWidth="1.7976931348623157E308" styleClass="textColor" text="TRAVELING SALESMAN PROBLEM" textAlignment="CENTER" wrapText="true" VBox.vgrow="NEVER">
                     <font>
                        <Font name="System Bold" size="42.0" />
                     </font>
                     <VBox.margin>
                        <Insets bottom="20.0" />
                     </VBox.margin>
                  </Label>
                  <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" VBox.vgrow="ALWAYS">
                     <image>
                        <Image url="@Icons/graph.png" />
                     </image>
                     <VBox.margin>
                        <Insets bottom="40.0" />
                     </VBox.margin>
                  </ImageView>
                  <HBox alignment="CENTER" maxWidth="1.7976931348623157E308">
                     <children>
                        <Button mnemonicParsing="false" onAction="#openMapDesigner" styleClass="buttonColor" text="NEW GAME" HBox.hgrow="ALWAYS">
                           <font>
                              <Font name="System Bold" size="31.0" />
                           </font>
                           <HBox.margin>
                              <Insets right="10.0" />
                           </HBox.margin>
                        </Button>
                        <Button mnemonicParsing="false" onAction="#openMapLoader" styleClass="buttonColor" text="LOAD GAME" HBox.hgrow="ALWAYS">
                           <font>
                              <Font name="System Bold" size="31.0" />
                           </font>
                           <HBox.margin>
                              <Insets left="10.0" />
                           </HBox.margin>
                        </Button>
                     </children>
                  </HBox>
               </children>
               <padding>
                  <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
               </padding>
            </VBox>
         </children>
      </HBox>
      <HBox layoutX="10.0" layoutY="10.0">
         <children>
            <CheckBox fx:id="darkMode" contentDisplay="TOP" minHeight="50.0" minWidth="50.0" mnemonicParsing="false" nodeOrientation="RIGHT_TO_LEFT" onAction="#setDarkMode" styleClass="darkmodeCheckbox" textFill="#ebe7e7">
               <font>
                  <Font size="18.0" />
               </font>
            </CheckBox>
         </children>
      </HBox>
   </children>
</AnchorPane>
