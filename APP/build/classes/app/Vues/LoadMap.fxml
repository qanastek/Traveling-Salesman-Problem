<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" fx:id="root" prefWidth="600.0" stylesheets="@stylesheet.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.Vues.LoadMapController">
   <children>
      <VBox alignment="CENTER" prefWidth="100.0" spacing="30.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox alignment="CENTER" prefWidth="200.0">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="customised-text" text="LOAD A MAP" textAlignment="CENTER" textOrigin="CENTER" HBox.hgrow="ALWAYS">
                     <font>
                        <Font size="34.0" />
                     </font>
                     <HBox.margin>
                        <Insets top="10.0" />
                     </HBox.margin>
                  </Text>
               </children>
            </HBox>
            <HBox spacing="20.0" VBox.vgrow="NEVER">
               <children>
                  <Button mnemonicParsing="false" onAction="#loadFrance" prefHeight="150.0" prefWidth="600.0" styleClass="btnOutlined" text="FRANCE" textFill="#fd9294" HBox.hgrow="NEVER">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#loadEiffelTower" prefHeight="150.0" prefWidth="600.0" styleClass="btnOutlined" text="EIFFEL TOWER" textFill="#fd9294" HBox.hgrow="NEVER">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#loadFile" prefHeight="150.0" prefWidth="600.0" styleClass="btnOutlined" text="LOAD FILE" textFill="#fd9294" HBox.hgrow="NEVER">
                     <font>
                        <Font size="25.0" />
                     </font>
                  </Button>
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" />
               </padding>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
